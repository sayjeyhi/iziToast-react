    },
    {
      "name": "Jafar Rezaei (sayjeyhi)",
      "email": "sayjeyhi@gmail.com",
      "homepage": "http://jrjs.ir"
    }
  ],
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
